<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.test.gui.EditCodePromo"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="34.0" layoutY="40.0" text="Value:" />
        <TextField fx:id="valueTextField" layoutX="120.0" layoutY="40.0" />
        <Label layoutX="34.0" layoutY="80.0" text="Code:" />
        <TextField fx:id="codeTextField" layoutX="120.0" layoutY="80.0" />
        <Label layoutX="34.0" layoutY="120.0" text="User:" />
        <ComboBox fx:id="userComboBox" layoutX="120.0" layoutY="120.0" />
        <Button layoutX="170.0" layoutY="160.0" mnemonicParsing="false" onAction="#saveChanges" text="Save Changes" />
    </children>
</AnchorPane>
